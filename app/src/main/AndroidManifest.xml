<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.project.monica.openreplyproject">

  <uses-permission android:name="android.permission.READ_CONTACTS" />
  <application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/AppTheme">
    <activity
      android:name=".activities.HomeActivity"
      android:label="@string/title_activity_home">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".activities.ProductDetailActivity"
      android:label="@string/title_activity_product_detail" />
    <activity
      android:name=".activities.ProductListActivity"
      android:label="@string/activity_name_basic_list" />
    <activity
      android:name=".activities.ProductListVarSizeActivity"
      android:label="@string/activity_name_var_list" />
    <activity
      android:name=".activities.ProductListSwipeActivity"
      android:label="@string/activity_name_swipe_list" />
  </application>

</manifest>
